{
  "sources": [
    {
      "folder": "svg/mutstd",
      "format": "svg",
      "filename": "{codepoint}.svg",
      "type": "non_qualified",
      "leading_zero": false,
      "lowercase": true,
      "default_skin": "-101622"
    },
    {
      "folder": "svg/blobmoji",
      "format": "svg",
      "filename": "emoji_u{codepoint}.svg",
      "replace": {
        "-": "_"
      },
      "type": "non_qualified",
      "leading_zero": true,
      "lowercase": true
    },
    {
      "folder": "svg/twemoji",
      "format": "svg",
      "filename": "{codepoint}.svg",
      "type": "non_qualified",
      "leading_zero": false,
      "lowercase": true
    }
  ],
  "settings": {
    "copy_input": "../app/javascript/mastodon/features/emoji/emoji_map.json",
    "sprite_input": "emoji_pretty.json",
    "size": 32,
    "destination": "../public/emoji",
    "sheet": "sheet_10.png"
  }
}